{
    "name": "marcoconsiglio/ephemeris",
    "description": "A Laravel package that query the Swiss Ephemeris executable, in order to get planets position for astronomy/astrology purposes.",
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MarcoConsiglio\\Ephemeris\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcoConsiglio\\Ephemeris\\Tests\\": "./tests/"
        }
    },
    "authors": [
        {
            "name": "Marco Consiglio",
            "email": "mrccnsgl@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MarcoConsiglio\\Ephemeris\\SwissEphemerisServiceProvider"
            ]
        }
    },
    "require": {
        "adambrett/shell-wrapper": "~1.0",
        "marcoconsiglio/goniometry": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "post-package-install": "php artisan vendor:publish",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "config": {
        "platform": {
            "php": "8.2"
        }
    }
}
